class Mytread extends Thread{

	@Override
	public void run() {
		// TODO Auto-generated method stub
		for(int i = 0; i < 100; i++)
		{
			System.out.println("mythread  "+i);
		}
	}
	
}

public class TestRun {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		MyThread thread = new MyThread();
		thread.start();
	}

}
