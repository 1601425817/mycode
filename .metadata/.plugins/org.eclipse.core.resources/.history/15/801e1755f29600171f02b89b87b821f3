import javax.sound.midi.Track;

class runThread extends Thread{

	@Override
	public void run() {
		for(int i = 0; i < 10000000; i++)
		{
			System.out.println("run  "+i);
		}
		try {
			Thread.sleep(5000);
			System.out.println("我执行完了");
		} catch (InterruptedException e) {
			// TODO Auto-generated catch block
			
			e.printStackTrace();
		}

	}
	
}
