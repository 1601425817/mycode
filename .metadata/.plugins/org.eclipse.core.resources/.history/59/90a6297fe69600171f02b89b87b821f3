
class runThread extends Thread{

	@Override
	public void run() {
		// TODO Auto-generated method stub
		System.out.println("1");
		
	}
	
}
public class TestRun {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		runThread mythread = new runThread();
		mythread.start();
		for(int i = 0; i < 100; i++)
		{
			System.out.println("main  "+i);
		}
	}

}
